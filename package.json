    "prisma": "^5.7.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
